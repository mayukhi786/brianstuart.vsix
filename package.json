{
    "name": "vscode-pets",
    "displayName": "VSCode Pets",
    "description": "Pets for your VSCode!",
    "icon": "./briandaddyicon.png",
    "repository": { "type": "git", "url": "https://github.com/mayukhi786/brianstuart.vsix.git" },
    "version": "1.0.5",
    "publisher": "Mayukhi Khan",
    "engines": {
        "vscode": "^1.59.0"
    },
    "categories": [
        "Education"
    ],
    "activationEvents": [
        "onView:vscode-pets.debugView"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "views": {
            "vscode-pets": [{
                "id": "vscode-pets.debugView",
                "name": "VSCode Pets",
                "type": "webview"
            }]
        },
        "viewsContainers": {
            "activitybar": [{
                "id": "vscode-pets",
                "title": "VSCode Pets",
                "icon": "./ddb.svg"
            }]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.2.2",
        "@types/node": "14.x",
        "@types/vscode": "^1.59.0",
        "@typescript-eslint/eslint-plugin": "^4.26.0",
        "@typescript-eslint/parser": "^4.26.0",
        "eslint": "^7.27.0",
        "glob": "^7.1.7",
        "mocha": "^10.0.0",
        "typescript": "^4.4.2",
        "vscode-test": "^1.5.2"
    }
}