{

    "name": "brian-stuart-pet",
    "displayName": "Brian Stuart Pet",
    "description": "Provide everyone with their own virtual duck in VS Code. Introduce students to rubber duck debugging, a technique that involves talking to a rubber duck (or any inanimate, or even animate object) about a bug in their code.",
    "icon": "images/briandaddyicon.png",
    "version": "1.0.6",
    "publisher": "Mayukhi Khan",
    "engines": {
        "vscode": "^1.59.0"
    },
    "categories": [
        "Education"
    ],
    "activationEvents": [
        "onView:ddb50.debugView"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "views": {
            "ddb50": [{
                        "id": "ddb50.debugView",
                        "name": "Brian Stuart Pet",

                        "ddb50": [{
                                "id": "ddb50.debugView",
                                "name": "Brian Stuart Pet",
                                "vscode-pets": [{
                                    "id": "vscode-pets.debugView",
                                    "name": "VSCode Pets",
                                    "type": "webview"
                                }]
                            },
                            "viewsContainers": {
                                "activitybar": [{
                                    "id": "ddb50",
                                    "title": "Brian Stuart Pet",
                                    "icon": "images/ddb.svg"
                                    "id": "ddb50",
                                    "title": "Brian Stuart Pet",
                                    "icon": "images/ddb.svg"
                                }]
                            }
                        },
                        "scripts": {
                            "vscode:prepublish": "npm run compile",
                            "compile": "tsc -p ./",
                            "watch": "tsc -watch -p ./",
                            "pretest": "npm run compile && npm run lint",
                            "lint": "eslint src --ext ts",
                            "test": "node ./out/test/runTest.js"
                        },
                        "devDependencies": {
                            "@types/glob": "^7.1.3",
                            "@types/mocha": "^8.2.2",
                            "@types/node": "14.x",
                            "@types/vscode": "^1.59.0",
                            "@typescript-eslint/eslint-plugin": "^4.26.0",
                            "@typescript-eslint/parser": "^4.26.0",
                            "eslint": "^7.27.0",
                            "glob": "^7.1.7",
                            "mocha": "^10.0.0",
                            "typescript": "^4.4.2",
                            "vscode-test": "^1.5.2"
                        }
                    }